{
    "name": "donkeycommerce/laravel-one-scopes",
    "description": "A package for manage role and scopes better. One mode for all auth providers, included passport.",
    "type": "library",
    "keywords": [
        "laravel",
        "token",
        "role",
        "scopes",
        "scope",
        "passport",
        "authentication",
        "authorization"
    ],
    "authors": [
        {
            "name": "danieletulone",
            "email": "danieletulone.work@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "danieletulone/laravel-files-support": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
            "DanieleTulone\\OneScopes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanieleTulone\\OneScopes\\Providers\\OneScopesServiceProvider"
            ]
        }
    }
}
